  <!-- Main Content -->
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-10 mx-auto">
        <% if(flashMessages.error != null && flashMessages.error.length > 0) {%>
          <ul class="list-group">
            <% for(var i = 0; i < flashMessages.error.length; i++){ %>
              <li class="list-group-item list-group-item-danger">
                <%= flashMessages.error[i] %>
              </li>
            <% } %>
          </ul>
        <% } %>
        <form action="/users/create" method="POST" enctype="multipart/form-data">
          <div class="control-group">
            <div class="form-group floating-label-form-group controls">
              <label>User Name</label>
              <input type="text" class="form-control" placeholder="User Name" id="username" name="username" value="<%= flashMessages.username %>">
            </div>
          </div>
          <div class="control-group">
            <div class="form-group floating-label-form-group controls">
              <label>Password</label>
              <input type="password" class="form-control" placeholder="Password" id="password" name="password" value="<%= flashMessages.password %>"">
            </div>
          </div>
          <br>
          <button type="submit" class="btn btn-primary" >Register</button>
        </form>
      </div>
    </div>
  </div>
